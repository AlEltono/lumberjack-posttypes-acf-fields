    "homepage": "https://github.com/ICEAgency/lumberjack-posttypes-acf-fields",
    "authors": [
        {
            "name": "Luke Hopkins",
            "email": "luke@theiceagency.co.uk",
            "homepage": "https://github.com/iceagency-lukehopkins"
        }
    ],
    "require": {
        "rareloop/lumberjack-core": "^3.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "brain/monkey": "^2.0.2",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
